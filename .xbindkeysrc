#increase volume
"amixer set Master playback 5%+; killall -USR1 i3status"
   m:0x0 + c:123
   XF86AudioRaiseVolume

#decrease volume
"amixer set Master playback 5%-; killall -USR1 i3status"
   m:0x0 + c:122
   XF86AudioLowerVolume

#toggle mute
"amixer set Master toggle; killall -USR1 i3status"
   m:0x0 + c:121
   XF86AudioMute
