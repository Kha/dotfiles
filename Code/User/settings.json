{
    "keyboard.dispatch": "keyCode",
    "workbench.colorTheme": "Solarized Light",
    "workbench.colorCustomizations": {
      "[Solarized Light]": {"editor.selectionHighlightBackground": "#e9dcc5"}
    },
    "editor.semanticTokenColorCustomizations": {
      "[Solarized Light]": {"rules": {"function": "#c1892f"}}
    },
    "editor.guides.indentation": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": [
          "<space>"
        ],
        "commands": [
          "vspacecode.space"
        ]
      },
      {
        "before": [
          ","
        ],
        "commands": [
          "vspacecode.space",
          {
            "command": "whichkey.triggerKey",
            "args": "m"
          }
        ]
      }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
      {
        "before": [
          "<space>"
        ],
        "commands": [
          "vspacecode.space"
        ]
      },
      {
        "before": [
          ","
        ],
        "commands": [
          "vspacecode.space",
          {
            "command": "whichkey.triggerKey",
            "args": "m"
          }
        ]
      }
    ],
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.minimap.enabled": false,
    "git.openRepositoryInParentFolders": "never",
    "vim.handleKeys": {
      "<C-p>": false,
      "<C-d>": true,
      "<C-s>": false,
      "<C-z>": false,
      "<C-f>": false
    },
    "search.useParentIgnoreFiles": true,
    "extensions.ignoreRecommendations": true,
    "whichkey.delay": 200,
    "vspacecode.bindingOverrides": [
      {
        "keys": "n",
        "name": "Notifications",
        "type": "bindings",
        "bindings": [
          {
            "key": "a",
            "name": "Accept notification primary action",
            "type": "command",
            "command": "notification.acceptPrimaryAction",
          },
          {
            "key": "c",
            "name": "Clear all notifications",
            "type": "command",
            "command": "notifications.clearAll",
          }
        ]
      },
      {
        "keys": "c.o",
        "name": "lake build --old",
        "type": "command",
        "command": "workbench.action.tasks.runTask",
        "args": "build-old",
      },
      {
        "keys": "m.languageId:lean4",
        "name": "Lean 4",
        "type": "bindings",
        "bindings": [
          {
            "key": "r",
            "name": "Refresh File Dependencies",
            "type": "command",
            "command": "lean4.refreshFileDependencies",
          },
          {
            "key": "R",
            "name": "Restart Server",
            "type": "command",
            "command": "lean4.restartServer",
          },
          {
            "key": "i",
            "name": "+Infoview",
            "type": "bindings",
            "bindings": [
              {
                "name": "Toggle Infoview",
                "type": "command",
                "command": "lean4.toggleInfoview",
              }
            ],
          },
          {
            "key": "g",
            "name": "+Go to",
            "icon": "go-to-file",
            "type": "bindings",
            "bindings": [
              {
                "key": "d",
                "name": "Go to definition",
                "icon": "symbol-function",
                "type": "command",
                "command": "editor.action.revealDefinition"
              },
              {
                "key": "g",
                "name": "Go to definition",
                "icon": "symbol-function",
                "type": "command",
                "command": "editor.action.revealDefinition"
              },
              {
                "key": "h",
                "name": "Show call hierarchy",
                "icon": "type-hierarchy",
                "type": "command",
                "command": "references-view.showCallHierarchy"
              },
              {
                "key": "r",
                "name": "Go to references",
                "icon": "symbol-reference",
                "type": "command",
                "command": "editor.action.goToReferences"
              },
              {
                "key": "t",
                "name": "Go to type definition",
                "icon": "symbol-struct",
                "type": "command",
                "command": "editor.action.goToTypeDefinition"
              },
              {
                "key": "R",
                "name": "Find references",
                "icon": "symbol-reference",
                "type": "command",
                "command": "references-view.findReferences"
              },
            ]
          },
        ],
      },
    ],
    "editor.rulers": [
      100
    ],
    "editor.fontFamily": "Iosevka",
    "editor.fontSize": 13,
    "explorer.confirmDelete": false,
    // https://github.com/microsoft/vscode/issues/184124
    "window.titleBarStyle": "custom",
    "workbench.editor.empty.hint": "hidden",
    "gitlens.advanced.messages": {
      "suppressCommitHasNoPreviousCommitWarning": true,
      "suppressCreatePullRequestPrompt": true
    },
    "window.zoomLevel": 1,
    "editor.stickyScroll.enabled": true,
    "editor.stickyTabStops": true,
    "githubPullRequests.pullBranch": "never",
    "cSpell.enableFiletypes": [
      "lean4"
    ],
    "cSpell.userWords": [
      "builtins",
      "Carneiro",
      "definitionally",
      "incrementality",
      "Mathlib",
      "Moura",
      "mvar",
      "Nawrocki",
      "subgoals",
      "Ullrich",
      "Wojciech"
    ],
    "cSpell.checkOnlyEnabledFileTypes": false,
    "cSpell.includeRegExpList": [
      "/--.*/",
      "/\\/-(.|\\n)*?-\\//",
      //"string"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "lean4.showSetupWarnings": false,
    "gitlens.launchpad.indicator.enabled": false,
    "git.replaceTagsWhenPull": true,
    "git.untrackedChanges": "separate",
    "githubPullRequests.createOnPublishBranch": "never",
    //"git.autofetch": "all",
    "githubPullRequests.pushBranch": "always",
    "cSpell.diagnosticLevel": "Hint",
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen"
    ],
    "terminal.integrated.defaultProfile.linux": "zsh (2)",
    "terminal.integrated.profiles.linux": {
      "bash": {
        "path": "bash",
        "icon": "terminal-bash"
      },
      "zsh": {
        "path": "zsh"
      },
      "fish": {
        "path": "fish"
      },
      "tmux": {
        "path": "tmux",
        "icon": "terminal-tmux"
      },
      "pwsh": {
        "path": "pwsh",
        "icon": "terminal-powershell"
      },
      "zsh (2)": {
        "path": "/usr/bin/zsh"
      }
    },
    "explorer.confirmDragAndDrop": false,
    //"terminal.integrated.stickyScroll.enabled": true,
    "extensions.experimental.affinity": {
      "vscodevim.vim": 1,
    },
    "remote.extensionKind": {
        "joelrobichaud.vscode-task-notifier": [ "ui" ],
        "jaredly.background-terminal-notifier": [ "ui" ],
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "terminal.integrated.allowedLinkSchemes": [
      "file",
      "http",
      "https",
      "mailto",
      "vscode",
      "vscode-insiders",
      "man"
    ],
    "github.copilot.enable": {
      "*": true,
      "plaintext": true,
      "markdown": false,
      "scminput": false,
      "lean4": true
    },
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "terminal.integrated.stickyScroll.enabled": false,
    "lean4.logging.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "chat.tools.terminal.autoApprove": {
      "lake build": {
        "approve": true,
        "matchCommandLine": true
      },
      "git add": true,
      "git commit": true,
      "^^^": true,
      "â‚‘": true
    },
    "terminal.integrated.suggest.enabled": false,
    "claudeCode.preferredLocation": "panel",
    //"lean4.serverArgs": [
    //  "-DstderrAsMessages=false"
    //],
    //"lean4.trace.server": "messages",
  }
