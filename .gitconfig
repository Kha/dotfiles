[core]
	excludesfile = ~/.gitignore
[user]
	email = sebasti@nullrich.de
	name = Sebastian Ullrich
[diff]
	guitool = kdiff3
[difftool]
	prompt = false
[alias]
	co = !git clean -Xfd && git checkout
	aa = commit -a -C HEAD --amend
	svnmerge = !git svn fetch && git merge remotes/git-svn
[merge]
	tool = kdiff3
